{
    "manifest_version": 3,
    "name": "将标签页另存为 MHTML 展示 (chrome.pageCapture)",
    "version": "1.0",
    "description": "展示 chrome.pageCapture API 相关功能",
    "icons":
    {
        "16": "images/icon.png",
        "48": "images/icon.png",
        "128": "images/icon.png"
    },
    "action": {
        "default_icon": "images/icon.png",
        "default_title": "展示 chrome.pageCapture API 相关功能",
        "default_popup": "pages/action.html"
    },
    "permissions": [
        "pageCapture",
        "tabs"
    ]
}