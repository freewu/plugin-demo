{
    "manifest_version": 3,
    "name": "搜索 展示 (chrome.search)",
    "version": "1.0",
    "description": "展示 chrome.search API",
    "icons":
    {
        "16": "images/icon.png",
        "48": "images/icon.png",
        "128": "images/icon.png"
    },
    "action": {
        "default_popup": "pages/action.html",
        "default_title": "搜索 展示 (chrome.search)",
        "default_icon": "images/icon.png"
    },
    "permissions": [
        "search"
    ]
}