{
    "manifest_version": 3,
    "name": "浏览器历史记录管理 展示 (chrome.history)",
    "version": "1.0",
    "description": "展示 chrome.history API",
    "icons":
    {
        "16": "images/icon.png",
        "48": "images/icon.png",
        "128": "images/icon.png"
    },
    "action": {
        "default_icon": "images/icon.png",
        "default_title": "展示 chrome.history API"
    },
    "background": {
        "service_worker": "js/background.js"
    },
    "permissions": [
        "history"
    ],
    "chrome_url_overrides": {
        "history": "pages/history.html"
    }
}