{
    "manifest_version": 3,
    "name": "键盘快捷键 展示 (chrome.commands)",
    "version": "1.0",
    "description": "展示 chrome.commands API",
    "icons":
    {
        "16": "images/icon.png",
        "48": "images/icon.png",
        "128": "images/icon.png"
    },
    "action": {
        "default_title": "键盘快捷键 展示 (chrome.commands)",
        "default_icon": "images/icon.png"
    },
    "background": {
        "service_worker": "js/background.js"
    },
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "commands": {
        "run-foo": {
            "suggested_key": {
                "default": "Ctrl+Shift+Up",
                "mac": "Command+Shift+Up"
            },
            "description": "Run \"foo\" on the current page."
        },
        "run-bar": {
            "suggested_key": "Ctrl+Shift+Down",
            "description": "Run \"bar\" on the current page."
        },
        "_execute_action": {
            "suggested_key": {
                "windows": "Ctrl+Shift+V",
                "mac": "Command+Shift+V",
                "chromeos": "Ctrl+Shift+V",
                "linux": "Ctrl+Shift+V"
            }
        }
    }
}